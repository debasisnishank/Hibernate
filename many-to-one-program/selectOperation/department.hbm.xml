<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="department" table="department" schema="nskdb">
 <id name="dept_id" type="java.lang.Integer">
 <column name="dept_id"  />
 <generator class="assigned" />
 </id>
 
 <property name="dept_name" type="java.lang.String">
 <column name="dept_name" length="50" />
 </property>
 
  <property name="dept_loc" type="java.lang.String">
 <column name="dept_loc" length="50"  />
 </property>
 


 
 
 
 
 </class>
</hibernate-mapping>